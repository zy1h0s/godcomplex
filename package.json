{
  "name": "overlay-collab-system",
  "version": "1.0.0",
  "description": "Real-time overlay collaboration system with web control panel and desktop overlay app",
  "scripts": {
    "install:all": "npm install --prefix backend && npm install --prefix frontend && npm install --prefix electron-app",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "dev:electron": "cd electron-app && npm start",
    "build:electron": "cd electron-app && npm run build",
    "build:electron:win": "cd electron-app && npm run build:win",
    "build:electron:mac": "cd electron-app && npm run build:mac",
    "build:electron:linux": "cd electron-app && npm run build:linux"
  },
  "keywords": [
    "overlay",
    "collaboration",
    "real-time",
    "electron",
    "react",
    "socket.io",
    "transparent-window"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/overlay-collab-system"
  }
}
